{
    "name":"color picker",
    "description":"A color picker extension by Rashid Iqbal",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"background.js"
    },
    "permissions":[
        "storage",
        "activeTab",
        "scripting"
    ],

    "options_page":"options.html",

    "action":{
        "default_popup":"popup.html"
    },

    "icons":{
        "16":"color-wheel.png"
    }
}
    
